{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "mocha": true,
        "es6": true
    },
    "rules": {
        "object-curly-newline": 0,
        "jsx-a11y/anchor-is-valid": 0,
        "react/jsx-closing-bracket-location": 0,
        "react/jsx-first-prop-new-line": 0,
        "max-len": 0,
        "no-plusplus": 0,
        "react/forbid-prop-types": 0
    }
}